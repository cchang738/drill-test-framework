select customer.c_custkey, customer.c_name, orders.o_orderkey, orders.o_totalprice from customer, lateral (select t.o.o_orderkey o_orderkey, t.o.o_totalprice o_totalprice from unnest(customer.c_orders) t(o) limit 2000) orders order by customer.c_custkey, orders.o_orderkey, orders.o_totalprice limit 50;
